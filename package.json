{
  "name": "pricesearcher-asmt",
  "version": "0.0.1",
  "description": "product search and display",
  "engines": {
    "node": "^8.11"
  },
  "scripts": {
    "clean": "config/clean.sh",
    "build": "tsc && node src/model/project/ProjectLoader.js && npx webpack --mode=production --config config/webpack.prod.js",
    "deploy": "yarn run build && firebase deploy",
    "jest": "jest",
    "reset": "config/reset_test.sh",
    "selenium": "config/selenium_install.sh",
    "staging": "tsc && node src/model/project/ProjectLoader.js && npx webpack --mode=production --config config/webpack.staging.js && config/remote_env_bundle.sh",
    "start": "npx webpack-dev-server --mode=development --config config/webpack.dev.js --open",
    "test": "config/run_tests.sh",
    "watch": "npx webpack --mode=development --watch --config config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "sideEffects": false,
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "^1.3.1",
    "@types/loglevel": "^1.5.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/underscore": "^1.8.8",
    "@types/underscore.string": "^0.0.32",
    "chromedriver": "^2.38.3",
    "create-react-class": "^15.6.2",
    "jest": "^23.1.0",
    "jest-environment-webdriver": "^0.2.0",
    "loglevel": "^1.6.0",
    "moment": "^2.21.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "underscore": "1.8.3",
    "underscore.string": "^3.3.4",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
